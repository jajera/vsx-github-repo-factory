{
  "name": "vsx-github-repo-factory",
  "displayName": "GitHub Repo Factory",
  "description": "Automate repository creation and setup",
  "version": "0.1.0",
  "publisher": "jajera",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:github-repo-factory.create",
    "onCommand:github-repo-factory.delete",
    "onCommand:github-repo-factory.modify"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "github-repo-factory.create",
        "title": "GitHub Repo Factory: Create Repository"
      },
      {
        "command": "github-repo-factory.delete",
        "title": "GitHub Repo Factory: Delete Repository"
      },
      {
        "command": "github-repo-factory.modify",
        "title": "GitHub Repo Factory: Modify Repository"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "test": "npm run test:compile && npm run test:unit",
    "test:unit": "mocha out/test/tests/**/*.test.js --timeout 10000",
    "test:compile": "tsc -p tsconfig.test.json",
    "test:integration": "npm run test:compile && node ./out/test/tests/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.0.0",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^21.0.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.3.0",
    "@vscode/vsce": "^3.7.0",
    "chai": "^4.3.10",
    "eslint": "^8.28.0",
    "glob": "^13.0.0",
    "mocha": "^10.8.2",
    "proxyquire": "^2.1.3",
    "sinon": "^21.0.0",
    "typescript": "^4.9.4"
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jajera/vsx-github-repo-factory.git"
  },
  "license": "MIT"
}
